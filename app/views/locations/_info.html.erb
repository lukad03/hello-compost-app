<div class="location-info">

  <section class="address">
    <h3>Address</h3>
    <p><%= @location.address %></p>
  </section>

  <section class="hours">
  </section>

  <section class="facilitators">
    <h3>Facilitators</h3>
    <% @location.facilitators.each do |facilitator| %>
      <p><%= facilitator.name %></p>
    <% end %>
  </section>
</div>
